<!DOCTYPE html>
<html>
  <head>
    <% include ../common/common-head.ejs %>
    <link rel='stylesheet' href='/stylesheets/thread/thread_list.css' />
  </head>
  <body>
    <% include ../common/common-nav.ejs %>
    <article>
        <ul id="threadList">
            <% for(var i=0, len=threads.length; i<len; i++) { %>
                <a href="/thread/thread?thread_id=<%= threads[i].thread_id %>"><li class="thread">
                    <p class="thread_title"><%= threads[i].title %></p>
                    <p class="thread_description"><%= threads[i].description %></p>
                </li></a>
            <% } %>
        </ul>
    </artilce>
    <% include ../common/common-foot.ejs %>
  </body>
</html>
